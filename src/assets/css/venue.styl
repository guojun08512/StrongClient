.add-venue
  margin-bottom 20px
.dialog-content
  .el-form-item
    width 100%
    &:first-child
      margin-top 20px
    .el-input
      width 85%
  .el-button
    padding 10px 25px
.venue-information
  background-color #fff
  text-align left
  &:nth-child(3)
    margin 30px 0
  .el-upload-list
    position none !important
  .upload-storelogo
    padding 30px
  h3
    border-bottom 1px solid #ccc
    padding 30px 0 30px 30px
    font-size 15px
    font-weight blod
  .info
    padding-bottom 20px
    .el-row
      display flex
      align-items center
      align-content center
      .el-col-8
        text-align center
        img
          width 200px
          height 200px
          border 1px solid #ccc
          display inline-block
          vertical-align middle
    ul li
      span
        font-size 14px
        &:first-child
          line-height 48px
          width 80px
          display inline-block
        .el-input
          width 320px
  .album
    display flex
    ul
      padding 0 20px 0 0
      margin 20px 0
      li
        width 200px
        height 200px
        display inline-block
        border 1px solid #ccc
        margin 0 5px
        img
          width 100%
          height 100%
          display inline-block
.describe
  margin-bottom 50px
  padding 0 30px
  h3
    padding 20px 0
    border-bottom 0 none
.message-notice
  background-color #fff
  border 1px solid #DCDEE2
  padding 35px
  display flex
  align-items center
  align-content center
  div
    &:first-child
      flex 0 0 20%
      border-right 2px solid #E8EAEC
      h3
        font-size 15px
        line-height 45px
      h1
        font-size 18px
        line-height 45px
    &:last-child
      padding-left 50px
      span
        color #3399FF
        padding 0 30px
        cursor pointer
.search-spanst
  .info
    display flex
    margin-top 15px
    align-items center
    img
      width 160px
      height 160px
      display inline-block
      border 1px solid #efefef
      margin-right 5px
    ul li
      float none
      line-height 20px
  .purchase-card
    padding 10px 0
    border-top 1px solid #E8EAEC
    border-bottom 1px solid #E8EAEC
    .no-carddata
      padding 20px 0
      p
        line-height 30px
        color #888
        font-size 14px
        width 100%
        text-align center
    .el-radio__inner
      background red !important
    &>div
      display flex
      flex-wrap wrap
      justify-content space-between
      ul
        margin 0 0 10px 0
        padding 0
        border-radius 5px
        border 1px solid #ccc
        flex 0 0 48%
        li
          padding 0 20px
    label.el-checkbox
      width 100%
      position relative
      span.el-checkbox__input
        position absolute !important
        right 20px !important
  .Hand-number-add
    margin-top 20px
    .el-form-item
      display block
      width auto
      .el-input
        width 60%
    .el-form-item__label
      width 50px
    .el-button
      padding 8px 13px
    .el-button+.el-button
      margin-left 10px
    div
      display inline-block
.signin-search
  .el-select
    display block
.face-main
  margin 20px 0
  .worker-info
    display flex
    .el-upload--picture-card
      line-height 148px
  .face-check
    margin-top 20px
    .check-title
      padding-bottom 4px
      border-bottom 1px solid #E8EAEC
      span
        font-size 14px
        font-weight 500
        color #2D8CF0
        padding-bottom 5px
        border-bottom 2px solid #2D8CF0
    .add-face
      display flex
      justify-content space-between
      padding 20px
      h3
        font-size 16px
      .el-button
        padding 10px 18px
.pornal-info
  width 100%
  li
    display flex
    span
      line-height 24px
      &:nth-child(1)
        flex 0 0 25%
.worker-info
  .el-upload
    img
      width 100%
      height 100%
      display inline-block
.division
  margin-bottom 20px
.perslAport
  .notice
    text-align center
    margin-bottom 10px
    p
      color #f40
      font-size 14px
      line-height 24px
      &:nth-child(2)
        text-indent 23px
      span
        color #888
        font-size 15px
  .el-form-item__label
    width 80px
.copy-class
  p
    text-align center
    font-size 15px
    line-height 32px
  label
    display block
    text-align center
    padding 10px 0 5px
p.cancel-order
  color #f40
  margin-bottom 30px
  text-indent 30px
.venue-infoList
  .venue-information
    .el-upload-list
      position absolute !important
      z-index 9
.el-radio__label
  li
    line-height 22px
.memberCard
  .el-form-item__label
    word-spacing 3px
.venue-information
  .album
    li
      box-shadow 0 2px 1px -1px rgba(0,0,0,.2),0 1px 1px 0 rgba(0,0,0,.14),0 1px 3px 0 rgba(0,0,0,.12)!important
      padding 8px
      position relative
      .el-icon-circle-close
        position absolute
        top 0
        right 0
        font-size 20px